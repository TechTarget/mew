html(lang='en', dir='ltr')
  head
    meta(name='pageStart', content='1359057078694')
    title Embedded Version of #{site.name}
    meta(http-equiv='Content-Type', content='text/html; charset=utf-8')
    link#stylesheetGlobal(rel='stylesheet', type='text/css', href='http://media.ttgtmedia.com/rms/ux/css/global.css', media='screen')
    link#stylesheetSiteSpecific(rel='stylesheet', type='text/css', href='http://media.ttgtmedia.com/rms/ux/css/searchsqlserver_new.css', media='screen')
    script(type='text/javascript', src='http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js')
